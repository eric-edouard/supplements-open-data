{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["created", "contributor", "paper", "symptom"],
  "properties": {
    "created": { "type": "string" },
    "contributor": { "type": "string" },
    "paper": { "type": "string" },
    "symptom": { "type": "string" },
    "incidence_percent": { "type": "number" },
    "paper_quotes": {
      "type": "object",
      "properties": {
        "abstract": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "paper_content": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      }
    }
  }
}
