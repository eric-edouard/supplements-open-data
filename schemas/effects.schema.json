{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": [
		"created",
		"contributor",
		"paper",
		"effect",
		"kind",
		"direction",
		"strength",
		"dosage_min",
		"dosage_max"
	],
	"properties": {
		"created": { "type": "string" },
		"contributor": { "type": "string" },
		"paper": { "type": "string" },
		"effect": { "type": "string" },
		"kind": { "enum": ["intended", "adverse", "neutral"] },
		"direction": { "enum": ["positive", "negative", "neutral"] },
		"description": { "type": "string" },
		"strength": { "enum": ["strong", "moderate", "weak", "none"] },
		"p_value": { "type": ["number", "string"] },
		"effect_size": { "type": "string" },
		"dosage_min": { "type": "number" },
		"dosage_max": { "type": "number" },
		"timing": { "type": "string" }
	}
}
