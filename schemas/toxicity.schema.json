{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"required": ["created", "contributor", "paper", "threshold_amount"],
	"properties": {
		"created": { "type": "string" },
		"contributor": { "type": "string" },
		"paper": { "type": "string" },
		"threshold_amount": { "type": "number" },
		"change": {
			"enum": [
				"negative",
				"slightly-negative",
				"neutral",
				"slightly-positive",
				"positive"
			]
		},
		"change_percent": { "type": "number" },
		"effect": { "type": "string" },
		"population": { "type": "string" }
	}
}
